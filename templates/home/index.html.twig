{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}
<div class="container">
    <h1>Hello</h1>
    <ul>
    {% for oneSpot in spots %}
        <li>
        <b>{{ oneSpot.title }} {{ oneSpot.content }}</b>
        <p>{{ oneSpot.author.nickname }}</p>
        <img src="{{ oneSpot.urlPos }}" alt="">
        </li>
    {% endfor %}
    </ul>
</div>
{% endblock %}
